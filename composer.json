{
    "name": "area17/cdn",
    "description": "CDN Cache Control and Invalidation",
    "keywords": [
        "area17",
        "laravel",
        "cdn"
    ],
    "homepage": "https://github.com/area17/cdn",
    "license": "MIT",
    "authors": [
        {
            "name": "AREA 17",
            "email": "support@area17.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "spatie/laravel-package-tools": "^1.4.3",
        "illuminate/contracts": "^8.37"
    },
    "require-dev": {
        "brianium/paratest": "^6.2",
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "phpunit/phpunit": "^9.3",
        "spatie/laravel-ray": "^1.9",
        "vimeo/psalm": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "Area17\\CDN\\": "src",
            "Area17\\CDN\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Area17\\CDN\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "./vendor/bin/testbench package:test --parallel --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Area17\\CDN\\CDNServiceProvider"
            ],
            "aliases": {
                "CDN": "Area17\\CDN\\CDNFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
